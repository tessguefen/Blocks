	</MvIF>

	<MvIF EXPR = "{ LayoutComponent_Load_ID( l.layout_value:layouts_components_id, l.layout_component ) }">
		<MvASSIGN NAME = "l.void" VALUE = "{ Layout_Cache_Delete( l.layout_component:layout_id ) }">
